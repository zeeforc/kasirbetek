private function parseAuthorString(string $author): array
{
if (Preg::isMatch('/^(?P<name>[- .,\p{L}\p{N}\p{Mn}\'Ã¢â‚¬â„¢"()]+)(?:\s+<(?P<email>.+?)>)?$/u', $author, $match)) {
