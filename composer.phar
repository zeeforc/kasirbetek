unset($this->config[$property]);

return false;
}

return true;
}





private function filterUrl($value, array $schemes = ['http', 'https']): bool
{
if ($value === '') {
return true;
}

$bits = parse_url($value);
if (empty($bits['scheme']) || empty($bits['host'])) {
return false;
}

if (!in_array($bits['scheme'], $schemes, true)) {
return false;
}

return true;
}
