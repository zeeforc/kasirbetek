{
if (extension_loaded('openssl')) {
return 'https';
}

return 'http';
}








protected function getContents(string $url): Response
{
$options = $this->repoConfig['options'] ?? [];

return $this->httpDownloader->get($url, $options);
}




public function cleanup(): void
{
}
