$watchLevel = $level;
}
}
}




public function getRule(): Rule
{
return $this->rule;
}







public function getOtherWatch(int $literal): int
{
if ($this->watch1 === $literal) {
return $this->watch2;
}

return $this->watch1;
}







public function moveWatch(int $from, int $to): void
{
if ($this->watch1 === $from) {
$this->watch1 = $to;
