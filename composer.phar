return [
'name' => trim($match['name']),
'email' => $match['email'],
];
}

throw new \InvalidArgumentException(
'Invalid author string.  Must be in the formats: '.
'Jane Doe or John Smith <john@example.com>'
);
}




protected function formatAuthors(string $author): array
{
